@model ErrorViewModel
@{
    ViewBag.Title = "Ошибка";
}

@if (Model.Errors.Count != 0)
{
    <ul class="ErrorLi">
        @foreach (var str in Model.Errors)
        {
            <li class="ErrorLi">@str</li>
        }
    </ul>
}

@if (!string.IsNullOrEmpty(Model.returnURL))
{
    <a href="@Model.returnURL" class="TFOMS_A">Назад</a>
}
